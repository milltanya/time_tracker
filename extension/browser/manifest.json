{
    "name": "Time Tracker Light",
    "description": "Time Tracker withour meta app",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
        "tabs",
        "storage",
        "http://localhost:8080/*",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
      },
      "default_title": "Click to Log In / Log Out"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png",
      "256": "images/icon256.png",
      "400": "images/icon.png"
    }
}