{
    "name": "Time Tracker",
    "description": "Time Tracker app",
    "version": "1.4",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "storage",
        "scripting"
    ],
    "host_permissions": [
        "http://localhost:8080/*",
        "http://*/",
        "https://*/"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "64": "images/icon64.png",
        "128": "images/icon128.png"
      },
      "default_title": "Click to Log In / Log Out"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "64": "images/icon64.png",
      "128": "images/icon128.png",
      "256": "images/icon256.png",
      "400": "images/icon.png"
    }
}